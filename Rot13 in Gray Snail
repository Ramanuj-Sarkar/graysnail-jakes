POP a rot13text a

"loop beginning"
OUTPUT "[rot13text]
This is the Gray Snail ROT-13 encoder.
Please enter text to be encoded."
INPUT plaintext
POP a rot13text a

"not done"
POP char plaintext [plaintext]
GOTO "rotA" [char] A
GOTO "rota" [char] a
GOTO "rotB" [char] B
GOTO "rotb" [char] b
GOTO "rotC" [char] C
GOTO "rotc" [char] c
GOTO "rotD" [char] D
GOTO "rotd" [char] d
GOTO "rotE" [char] E
GOTO "rote" [char] e
GOTO "rotF" [char] F
GOTO "rotf" [char] f
GOTO "rotG" [char] G
GOTO "rotg" [char] g
GOTO "rotH" [char] H
GOTO "roth" [char] h
GOTO "rotI" [char] I
GOTO "roti" [char] i
GOTO "rotJ" [char] J
GOTO "rotj" [char] j
GOTO "rotK" [char] K
GOTO "rotk" [char] k
GOTO "rotL" [char] L
GOTO "rotl" [char] l
GOTO "rotM" [char] M
GOTO "rotm" [char] m
GOTO "rotN" [char] N
GOTO "rotn" [char] n
GOTO "rotO" [char] O
GOTO "roto" [char] o
GOTO "rotP" [char] P
GOTO "rotp" [char] p
GOTO "rotQ" [char] Q
GOTO "rotq" [char] q
GOTO "rotR" [char] R
GOTO "rotr" [char] r
GOTO "rotS" [char] S
GOTO "rots" [char] s
GOTO "rotT" [char] T
GOTO "rott" [char] t
GOTO "rotU" [char] U
GOTO "rotu" [char] u
GOTO "rotV" [char] V
GOTO "rotv" [char] v
GOTO "rotW" [char] W
GOTO "rotw" [char] w
GOTO "rotX" [char] X
GOTO "rotx" [char] x
GOTO "rotY" [char] Y
GOTO "roty" [char] y
GOTO "rotZ" [char] Z
GOTO "rotz" [char] z
POP a rot13text a[rot13text][char]
"char rotated"
OUTPUT [rot13text]
GOTO "loop beginning" [plaintext] ""
GOTO "not done" a a

